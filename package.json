{
  "name": "@crepen-cdn/repo",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "cloud": "yarn workspace @crepen-cdn/cloud",
    "server": "yarn workspace @crepen-cdn/server",
    "admin": "yarn workspace @crepen-cdn/admin"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.9.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
